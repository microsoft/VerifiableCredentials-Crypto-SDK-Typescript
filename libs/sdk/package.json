    "lint": "tslint --fix --project . --config ./tslint.json",
    "lint-pr": "tslint --project . --config ./tslint.json"
  },
  "devDependencies": {
    "@types/jasmine": "^2.8.9",
    "@types/node": "14.6.2",
    "@types/uuid": "3.4.4",
    "jasmine": "3.6.3",
    "jasmine-reporters": "^2.3.2",
    "jasmine-spec-reporter": "^6.0.0",
    "jasmine-ts": "^0.3.0",
    "nyc": "^15.1.0",
    "prettier": "^1.18.2",
    "ts-node": "8.5.4",
    "tslint": "^5.20.0",
    "typescript": "4.0.3"
  },
  "dependencies": {
    "@azure/identity": "1.0.0",
    "@azure/keyvault-keys": "4.0.2",
    "@azure/keyvault-secrets": "4.0.2",
    "@decentralized-identity/ion-sdk": "0.4.2",
    "base64url": "3.0.1",
    "bs58": "4.0.1",
