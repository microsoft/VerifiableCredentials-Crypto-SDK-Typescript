    "verifiablecredentials-crypto-sdk-typescript-protocol-jose": "1.1.12-preview.7",
    "verifiablecredentials-crypto-sdk-typescript-protocols-common": "1.1.12-preview.7",
    "webcrypto-core": "1.1.8"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "include": [
      "lib/**"
    ],
    "reporter": [
      "text",
      "cobertura",
      "html"
