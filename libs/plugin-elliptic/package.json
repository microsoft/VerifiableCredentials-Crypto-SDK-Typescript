{
  "name": "verifiablecredentials-crypto-sdk-typescript-plugin-elliptic",
  "version": "1.1.11-preview.3",
  "repository": {
    "type": "git",
    "url": "https://github.com/microsoft/VerifiableCredentials-Crypto-SDK-Typescript.git"
  },
  "description": "Subtle crypto plugin to support elliptic curve operations.",
  "keywords": [
    "Decentralized Identity",
    "Verifiable Credentials",
    "DID",
    "crypto",
    "keys"
  ],
  "author": "Microsoft Corporation",
  "license": "MIT",
  "main": "dist/src/index.js",
  "scripts": {
    "build": "tsc --p tsconfig.json",
    "test": "nyc jasmine-ts --config=./tests/jasmine.json",
    "lint": "tslint --fix --project . --config ./tslint.json",
    "lint-pr": "tslint --project . --config ./tslint.json"
  },
  "dependencies": {
    "base64url": "3.0.1",
    "bn.js": "^4.11.8",
    "clone": "2.1.2",
    "elliptic": "6.5.3",
    "minimalistic-crypto-utils": "1.0.1",
    "sha.js": "^2.4.11",
    "verifiablecredentials-crypto-sdk-typescript-plugin": "1.1.11-preview.3",
    "webcrypto-core": "1.1.1"
  },
  "devDependencies": {
    "@types/jasmine": "2.8.16",
    "@types/node": "12.12.16",
    "husky": "0.14.3",
    "jasmine": "3.5.0",
    "jasmine-reporters": "2.3.2",
    "jasmine-spec-reporter": "4.2.1",
    "jasmine-ts": "0.2.1",
    "ts-loader": "2.3.7",
    "node-fetch": "2.4.1",
    "nyc": "14.1.1",
    "source-map-support": "0.5.12",
    "ts-node": "8.5.4",
    "tslint": "^5.20.0",
    "tslint-microsoft-contrib": "6.1.1",
    "typescript": "3.9.2"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "include": [
      "src/**"
    ],
    "reporter": [
      "text",
      "cobertura",
      "html"
    ]
  },
  "files": [
    "dist/**",
    "lib/**",
    "README.md",
    "LICENSE",
    "package.json"
  ]
}
